<VirtualHost *:8001>
    #ServerName 172.26.74.174
    CustomLog {{runner.logs_folder}}/accesslog common
    ErrorLog {{runner.logs_folder}}/errorlog
    WSGIDaemonProcess {{runner.qualified_project_name}} python-path={{runner.current_folder}} python-home={{runnner.venv_folder}} home={{runner.current_folder}} user={{runner.run_as}}
    <Directory {{runner.current_folder}}>
    <Files wsgi.py>
        Require all granted
    </Files>
    </Directory>
</VirtualHost>
